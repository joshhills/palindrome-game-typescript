    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.2",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^10.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
