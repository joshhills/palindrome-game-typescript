{
  "name": "palindrome-game",
  "version": "1.0.0",
  "description": "A simple palindrome game with hiscores using Node.js",
  "scripts": {
    "start:dev": "nodemon dist/index.js",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "dev": "concurrently \"npm:build:dev\" \"npm:start:dev\""
  },
  "keywords": [
    "palindrome",
    "node",
    "express"
  ],
  "author": "Josh Hills",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^13.7.0",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2",
    "typescript": "^3.7.5"
  }
}
